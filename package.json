  "main": "index.js",
  "scripts": {
    "postinstall-dev": "([ \"$NODE_ENV\" != production ] && (npm run build && npm run build-backend)) || echo $NODE_ENV",
    "build-backend": "rm -f server.build.js && webpack --config webpack.backend.config.js --progress --profile --colors",
    "build": "webpack --config webpack.production.config.js --progress --profile --colors",
    "start": "node server.build.js",
    "start-debug": "babel-node --inspect server.js",
    "start-dev": "webpack-dev-server --progress --profile --colors",
    "lint": "eslint --ext js --ext jsx src || exit 0",
    "dev": " webpack-dashboard -- webpack-dev-server --progress --profile --colors",
    "postinstall": "npm run postinstall-dev & db-migrate up"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/audioasyl/website.git"
